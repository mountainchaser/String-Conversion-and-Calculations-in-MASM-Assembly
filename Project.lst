				; Receives: {parameters: progTitle (reference), programmer (reference), ec1 (reference), ec2 (reference),
				;			progDesc(reference)}
				;
				; Returns: None
				; ---------------------------------------------------------------------------------------------------

					mDisplayString	MACRO	stringAddress
						;print stored string using WriteString
						MOV		EDX, stringAddress
						CALL	WriteString
					ENDM

